{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": ["error", { "functions": true, "classes": true }],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
    "indent": ["error", 4],
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-boolean-value": 0,
    "comma-dangle": 0
  }
}
